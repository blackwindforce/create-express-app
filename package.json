{
  "name": "create-express-app",
  "version": "1.0.0",
  "description": "create-express-app",
  "homepage": "https://github.com/blackwindforce/create-express-app#readme",
  "bugs": {
    "url": "https://github.com/blackwindforce/create-express-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackwindforce/create-express-app.git"
  },
  "license": "MIT",
  "author": "Akira Cai",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "cspell --no-summary",
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "cspell --no-summary",
      "prettier --write"
    ],
    "*.yml": [
      "cspell --no-summary",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "env": {
      "es2021": true,
      "jest": false
    },
    "extends": [
      "eslint:recommended",
      "standard",
      "plugin:jest/recommended",
      "plugin:prettier/recommended",
      "prettier/standard"
    ],
    "overrides": [
      {
        "files": [
          "*.spec.js"
        ],
        "env": {
          "jest": true
        }
      }
    ],
    "root": true
  },
  "jest": {
    "coverageReporters": [
      "text"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "fluture": "^13.0.1",
    "fp-ts": "^2.9.1",
    "lodash": "^4.17.20",
    "ramda": "^0.27.1",
    "rxjs": "^6.6.3",
    "sanctuary": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.16",
    "commitizen": "^4.2.2",
    "cspell": "^4.2.5",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.4",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": "^14.15.1",
    "npm": "^6.14.8"
  }
}
