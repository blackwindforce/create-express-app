{
  "name": "create-express-app",
  "version": "1.0.0",
  "description": "create-express-app",
  "homepage": "https://github.com/blackwindforce/create-express-app#readme",
  "bugs": {
    "url": "https://github.com/blackwindforce/create-express-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackwindforce/create-express-app.git"
  },
  "license": "MIT",
  "author": "Akira Cai",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "cspell --no-summary",
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "cspell --no-summary",
      "prettier --write"
    ],
    "*.yml": [
      "cspell --no-summary",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "env": {
      "es2020": true,
      "jest": false
    },
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "eslint-config-standard",
      "plugin:eslint-plugin-jest/recommended",
      "eslint-config-prettier",
      "eslint-config-prettier/standard"
    ],
    "overrides": [
      {
        "files": [
          "*.spec.js"
        ],
        "env": {
          "jest": true
        }
      }
    ],
    "root": true
  },
  "jest": {
    "coverageReporters": [
      "text"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "fluture": "^13.0.1",
    "fp-ts": "^2.7.1",
    "lodash": "^4.17.20",
    "ramda": "^0.27.1",
    "rxjs": "^6.6.3",
    "sanctuary": "^3.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.2.2",
    "cspell": "^4.1.2",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.14.0",
    "eslint-config-standard": "^15.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.2",
    "husky": "^4.3.0",
    "jest": "^26.6.1",
    "lint-staged": "^10.4.2",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": "^12.19.0",
    "npm": "^6.14.8"
  }
}
